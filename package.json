{
  "name": "angular2-skeleton",
  "version": "1.0.0",
  "description": "A skeleton Angular2 project with TypeScript, Bootstrap and Sass",
  "scripts": {
    "start": "npm run build",
    "prebuild": "npm run lint",
    "build": "concurrently \"npm run tsc:w\" \"npm run sass:w\" \"npm run server\"",
    "lint": "tslint scripts/**.ts",
    "tsc": "tsc scripts/**.ts",
    "tsc:w": "tsc -w scripts/**.ts",
    "sass": "sass styles/sass/style.scss:styles/style.css",
    "sass:w": "sass styles/sass/style.scss:styles/style.css -w",
    "server": "live-server --port=3000",
    "postinstall": "typings install",
    "typings": "typings"
  },
  "author": "Christopher Ford",
  "license": "ISC",
  "dependencies": {
    "@angular/common":  "2.0.0-rc.1",
    "@angular/compiler":  "2.0.0-rc.1",
    "@angular/core":  "2.0.0-rc.1",
    "@angular/http":  "2.0.0-rc.1",
    "@angular/platform-browser":  "2.0.0-rc.1",
    "@angular/platform-browser-dynamic":  "2.0.0-rc.1",
    "@angular/router":  "2.0.0-rc.1",
    "@angular/router-deprecated":  "2.0.0-rc.1",
    "@angular/upgrade":  "2.0.0-rc.1",
    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "angular2-in-memory-web-api": "0.0.11",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "live-server": "^1.0.0",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "watch": "^0.18.0",
    "typings": "^1.0.4"
  }
}
